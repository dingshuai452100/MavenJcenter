<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="xrv_SpringView">
        <attr name="xrv_type">
            <enum name="overlap" value="0" />
            <enum name="follow" value="1" />
            <enum name="drag" value="2" />
        </attr>
        <attr name="xrv_give">
            <enum name="both" value="0" />
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
            <enum name="none" value="3" />
        </attr>
        <attr name="xrv_header" format="reference" />
        <attr name="xrv_footer" format="reference" />
    </declare-styleable>
</resources>